    "nuxt": "^3.7.3",
    "nuxt-simple-sitemap": "^3.3.5",
    "nuxt-vite": "^0.3.5",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.4",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
